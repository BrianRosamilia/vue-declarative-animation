    "babel-eslint": "^10.0.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "cross-env": "^5.2.0",
    "eslint": "^4.10.0",
    "eslint-plugin-vue": "^4.7.1",
    "express": "^4.15.3",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.11.0",
    "prismjs": "^1.15.0",
    "sass": "^1.16.0",
    "vue-hot-reload-api": "^2.3.1",
